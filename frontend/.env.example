# Frontend Environment Variables
# Copy this file to .env and update with your values

# API Base URL - Your backend server URL
# For local development: http://localhost:5000
# For production: http://YOUR_SERVER_IP or https://yourdomain.com
VITE_API_BASE_URL=http://localhost:5000

# Hosted mode (set to "true" for production, "false" for development)
# Optional - will auto-detect based on build mode if not set
VITE_HOSTED=false
